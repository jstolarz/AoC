rotations = []
with open(0) as f:
    for line in f.read().splitlines():
        if line[0] == "R":
            rotations.append(int(line[1:]))
        elif line[0] == "L":
            rotations.append(-int(line[1:]))

point_at_zero = 0
position = 50

for rotation in rotations:
    position = (position + rotation) % 100
    if position == 0:
        point_at_zero += 1
print(point_at_zero)
