rotations = []
with open(0) as f:
    for line in f.read().splitlines():
        if line[0] == "R":
            rotations.append(int(line[1:]))
        elif line[0] == "L":
            rotations.append(-int(line[1:]))

point_at_zero = 0
position = 50

for rotation in rotations:
    new_position = position + rotation
    if rotation > 0:
        if new_position >= 100:
            point_at_zero += new_position // 100
    else:
        if new_position <= 0:
            point_at_zero += (new_position // -100) + (1 if position > 0 else 0)
    position = new_position % 100
